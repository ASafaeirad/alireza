{
  "name": "alireza",
  "version": "1.3.3",
  "main": "index.js",
  "keywords": [
    "cv",
    "cli"
  ],
  "author": "frontendmonster <frontendmonster@gmail.com>",
  "license": "MIT",
  "description": "the cli version of my CV",
  "files": [
    "lib/",
    "README.md"
  ],
  "bin": {
    "alireza": "./lib/index.js"
  },
  "scripts": {
    "clean": "shx rm -rf ./lib",
    "lint": "eslint ./src --ext .ts,.tsx --fix",
    "prebuild": "npm run clean",
    "build": "tsc",
    "start": "ts-node-dev --transpile-only src/index.tsx",
    "prerelease": "npm run build",
    "release": "np",
    "spell": "cspell ./src/{**/*,*} --no-progress",
    "verify": "npm run lint && npm run spell"
  },
  "dependencies": {
    "console-clear": "1.1.1",
    "ink": "3.2.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@fullstacksjs/eslint-config": "8.9.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "cspell": "6.14.3",
    "eslint": "8.28.0",
    "np": "7.6.2",
    "prettier": "2.8.0",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.3"
  },
  "np": {
    "testScript": "verify",
    "cleanup": false
  }
}
