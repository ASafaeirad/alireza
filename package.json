{
  "name": "alireza",
  "version": "1.3.3",
  "main": "index.js",
  "keywords": [
    "cv",
    "cli"
  ],
  "author": "ASafaeirad <frontendmonster@gmail.com>",
  "license": "MIT",
  "description": "The cli version of my CV",
  "files": [
    "lib/",
    "README.md"
  ],
  "type": "module",
  "bin": {
    "alireza": "./lib/index.js"
  },
  "scripts": {
    "lint": "eslint ./src --ext .ts,.tsx --fix",
    "prebuild": "shx rm -rf ./lib",
    "build": "tsc",
    "start": "node --loader ts-node/esm --watch src/index.tsx",
    "prerelease": "npm run build",
    "release": "np",
    "spell": "cspell ./src/{**/*,*} --no-progress",
    "verify": "npm run lint && npm run spell"
  },
  "dependencies": {
    "console-clear": "1.1.1",
    "ink": "4.2.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@fullstacksjs/eslint-config": "9.8.0",
    "@sindresorhus/tsconfig": "3.0.1",
    "@types/console-clear": "1.1.0",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "cspell": "6.31.1",
    "eslint": "8.44.0",
    "np": "8.0.4",
    "prettier": "2.8.8",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "np": {
    "testScript": "verify",
    "cleanup": false
  },
  "engines": {
    "node": ">=16"
  }
}
